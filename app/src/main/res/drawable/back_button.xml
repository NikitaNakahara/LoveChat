<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="40dp"
        android:height="40dp">
        <shape>
            <solid android:color="@color/window_bg"/>
            <corners android:radius="20dp"/>
        </shape>
    </item>
    <item
        android:width="30dp"
        android:height="20dp"
        android:gravity="center">
        <bitmap android:src="@drawable/back_arrow"
            android:tint="@color/contrast_color"/>
    </item>
</layer-list>